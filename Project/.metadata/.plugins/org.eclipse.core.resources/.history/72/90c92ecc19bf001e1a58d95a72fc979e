package ch02_pjt_01;

public class MyCalculator {
	
	public void calAdd(int fNum, int sNum, CalAddImpl ca) {
		int value = ca.doOperation(fNum,sNum);
		System.out.println("result : " + value);
	}
	
	public void calSub(int fNum, int sNum, CalSubImpl ca) {
		int value = ca.doOperation(fNum,sNum);
		System.out.println("result : " + value);
	}
	
	public void calMul(int fNum, int sNum, CalMulImpl ca) {
		int value = ca.doOperation(fNum,sNum);
		System.out.println("result : " + value);
	}
	
	public void calDiv(int fNum, int sNum, CalDivImpl ca) {
		int value = ca.doOperation(fNum,sNum);
		System.out.println("result : " + value);
	}
}